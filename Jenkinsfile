pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
               sh 'echo "This is my first step"' 
            }
        }
        stage('Test') {
            steps {
                sh 'echo "This is my Test step"' 
            }
        }
        stage('Package') {
            steps {
                sh 'echo "This is my package step"' 
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "This is my Deploy step"' 
            }
        }
    }
}
